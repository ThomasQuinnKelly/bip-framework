		      printMethod="gov.va.bip.framework.transfer.jaxb.adapters.DateAdapter.printDate"
		    />
		<jaxb:javaType name="java.util.Date" xmlType="xs:dateTime"
		      parseMethod="gov.va.bip.framework.transfer.jaxb.adapters.DateAdapter.parseDate"
		      printMethod="gov.va.bip.framework.transfer.jaxb.adapters.DateAdapter.printDate"
		    />
	</jaxb:globalBindings>
	
</jaxb:bindings>